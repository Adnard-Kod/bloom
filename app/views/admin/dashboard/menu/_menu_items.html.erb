              <div class="form-group">
                <label>Item Description</label>
                <textarea class="form-control" rows="3"></textarea>
              </div>

              <div class="form-group">
                <label>Classifications</label>
                <label class="checkbox-inline">
                  <input type="checkbox">Entrée
                </label>
                <label class="checkbox-inline">
                  <input type="checkbox">Side Dish
                </label>
                <label class="checkbox-inline">
                  <input type="checkbox">Add On Option
                </label>
              </div>

              <button type="submit" class="btn btn-default">Create Item</button>
              <button type="reset" class="btn btn-default">Reset Item</button>

            </form>

          </div>

          <div class="col-lg-12">
            <div class="page-header">
              <h2>Entrées</h2>
